{
  "name": "ngx-coins-invest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --e=local --port 4300 --host 0.0.0.0 --disableHostCheck true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "static-serve": "cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html",
    "build": "ng build",
    "precach": "sw-precache --verbose --config=sw-precache-config.js",
    "******BUILD_TASKS": "*************************BUILDS***********************",
    "build:dev": "ng build --prod --e=dev && npm run precach",
    "build:prod": "ng build --prod --e=prod && npm run precach",
    "******UPLOAD_TASKS": "************************UPLOADS************************",
    "uploadTo:prod": "aws-fed-deployment --dpl-config config/deploy.prod.config.json",
    "uploadTo:dev": "aws-fed-deployment --dpl-config config/deploy.dev.config.json",
    "******DELIVERY_TASKS": "**********************DELIVERY*************************",
    "delivery:prod": "npm run build:prod && npm run uploadTo:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-webworker": "^5.0.0",
    "@angular/platform-webworker-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/effects": "^4.1.0",
    "@ngrx/entity": "^4.1.0",
    "@ngrx/router-store": "^4.1.0",
    "@ngrx/store": "^4.1.0",
    "@ngrx/store-devtools": "^4.0.0",
    "amazon-cognito-identity-js": "^1.21.0",
    "angular2-universal": "^2.1.0-rc.1",
    "autobahn": "^17.5.2",
    "aws-fed-deployment": "0.1.0",
    "body-parser": "^1.18.2",
    "chart.js": "^2.7.1",
    "core-js": "^2.4.1",
    "d3": "^4.10.2",
    "express": "^4.16.2",
    "live-server": "^1.2.0",
    "ng2-charts": "^1.6.0",
    "preboot": "^5.1.7",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.6",
    "socket.io-client": "^2.0.4",
    "sw-precache": "^5.2.0",
    "ts-helpers": "^1.1.2",
    "web-animations-js": "2.2.5",
    "webpack-dev-server": "^2.9.4",
    "zone.js": "^0.8.4"
