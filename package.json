{
    "name": "ngx-coins-invest",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --e=local --port 4300 --host 0.0.0.0 --disableHostCheck true",
        "start:ssl": "ng serve --ssl --ssl-key key.pem --ssl-cert cert.pem --e=local --port 43001 --host 0.0.0.0",
        "start:prod": "ng serve --e=prod --port 4300 --host 0.0.0.0 --disableHostCheck true",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "static-serve": "cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html",
        "build": "ng build",
        "precach": "sw-precache --verbose --config=sw-precache-config.js",
        "******BUILD_TASKS": "*************************BUILDS***********************",
        "build:dev": "ng build --prod --e=dev && npm run precach",
        "build:prod": "ng build --prod --aot false",
        "******UPLOAD_TASKS": "************************UPLOADS************************",
        "uploadTo:prod": "aws-fed-deployment --dpl-config config/deploy.prod.config.json",
        "uploadTo:dev": "aws-fed-deployment --dpl-config config/deploy.dev.config.json",
        "******DELIVERY_TASKS": "**********************DELIVERY*************************",
        "delivery:prod": "npm run build:prod && npm run uploadTo:prod",
        "ngsConfig": "ngsw-config dist ./src/ngsw-config.json",
        "http-server": "http-server ./dist -p 7090"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.1.2",
        "@angular/cdk": "^5.0.2",
        "@angular/common": "^5.1.2",
        "@angular/compiler": "^5.1.2",
        "@angular/core": "^5.1.2",
        "@angular/forms": "^5.1.2",
        "@angular/http": "^5.1.2",
        "@angular/material": "^5.0.0",
        "@angular/platform-browser": "^5.1.2",
        "@angular/platform-browser-dynamic": "^5.1.2",
        "@angular/platform-webworker": "^5.1.2",
        "@angular/platform-webworker-dynamic": "^5.1.2",
        "@angular/router": "^5.1.2",
        "@angular/service-worker": "^5.1.2",
        "@ngrx/effects": "^4.1.1",
        "@ngrx/entity": "^4.1.1",
        "@ngrx/router-store": "^4.1.1",
        "@ngrx/store": "^4.1.1",
        "@ngrx/store-devtools": "^4.1.1",
        "amazon-cognito-identity-js": "1.21.0",
        "angulartics2": "^4.0.1",
        "autobahn": "^17.5.2",
        "aws-fed-deployment": "0.1.0",
        "body-parser": "^1.18.2",
        "chart.js": "^2.7.1",
        "core-js": "^2.4.1",
        "d3": "^4.10.2",
        "express": "^4.16.2",
        "live-server": "^1.2.0",
        "ng2-charts": "^1.6.0",
        "preboot": "^5.1.7",
        "rxjs": "^5.5.2",
        "sass-loader": "^6.0.6",
        "socket.io-client": "^2.0.4",
        "sw-precache": "^5.2.0",
        "ts-helpers": "^1.1.2",
        "web-animations-js": "2.2.5",
        "webpack-dev-server": "^2.9.4",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "^1.6.0",
        "@angular/compiler-cli": "^5.1.2",
        "@angular/language-service": "^5.1.2",
        "@types/aws-sdk": "^2.7.0",
        "@types/d3": "^4.10.1",
        "@types/jasmine": "2.5.38",
        "@types/node": "^6.0.90",
        "codelyzer": "~2.0.0",
        "http-server": "^0.10.0",
        "jasmine-core": "~2.5.2",
        "jasmine-spec-reporter": "~3.2.0",
        "jasmine-testrail-reporter": "^1.0.1",
        "karma": "~1.4.1",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^0.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~2.0.0",
        "tslint": "~4.5.0",
        "typescript": "^2.4.2"
    },
    "license": "MIT"
}
